<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>RM PRO</title>

    <!-- IMPORT BOXICONS -->
    <link href='https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css' rel='stylesheet'>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <link href="~/css/bootstrap.min.css" rel="stylesheet" />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@3.4.1/dist/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="https://cdn.datatables.net/2.0.3/css/dataTables.dataTables.css" />
    <link rel="stylesheet" href="https://cdn.datatables.net/buttons/3.0.1/css/buttons.dataTables.min.css" />
    <link href="~/css/jquery.inputpicker.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />

    <script type="text/javascript" src="https://fastly.jsdelivr.net/npm/echarts@5.5.0/dist/echarts.min.js"></script>

</head>
<body>

    <aside class="active">
        <!--close / active-->

        <div class="head">
            <div class="logo">
                <img src="~/images/rm_logo.png" style="width: 100%; height:100%" alt="RM Logo">

            </div>
            <i class='bx bx-menu hamburger-menu'></i>
        </div>

        <div class="nav">
             <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Dashboard">
            <div class="menu">
                <i class="bx bx-pie-chart-alt"></i>
                <span>Dashboard</span>
            </div>
            </a>
            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Enquiry">
            <div class="menu">
               
                <i class='bx bx-shopping-bag'></i>
                    <span>Enquiry</span>
               
            </div>
            </a>

            <div class="menu">
                <i class="bx bx-task"></i>
                <span>Master</span>
                <i class="bx bx-chevron-down"></i>
            </div>
            <div class="menu-dropdown">
                <div class="sub-menu">
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="GeneralMaster">  <span class="menu">General Master</span></a>
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="ProductMaster">  <span class="menu">Product Master</span></a>
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="ProductCompositionMaster">  <span class="menu">Product Composition Master</span></a>
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="ProductWeightMaster">  <span class="menu">Product Weight Master</span></a>
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="VendorMaster">  <span class="menu">Vendor Master</span></a>
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="CustomerMaster">  <span class="menu">Customer Master</span></a>
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="CommodityExchangeCombination">  <span class="menu">Commodity Exchange Combination</span></a>
                   
                </div>
            </div>


            <div class="menu">
                <i class='bx bx-import'></i>
                <span>Data Import</span>
                <i class="bx bx-chevron-down"></i>
            </div>
            <div class="menu-dropdown">
                <div class="sub-menu">
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="MasterImport">  <span class="menu">Master Import</span></a>

                    <span class="menu">Transaction Import</span>
                </div>
            </div>

            <a class="nav-link" asp-area="" asp-controller="Home" asp-action="ShouldCostEnquiry">
                <div class="menu">
                    <i class="bx bx-pie-chart-alt"></i>
                    <span>Should Costing</span>
                </div>
            </a>

            <div class="menu">
                <i class='bx bxs-report'></i>
                <span>Reports</span>
                <i class="bx bx-chevron-down"></i>
            </div>
            <div class="menu-dropdown">
                <div class="sub-menu">
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="PriceCalcReport">  <span class="menu">Price Calc Report</span></a>
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="ShouldCostReport">  <span class="menu">Should Cost Report</span></a>
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="BOM">  <span class="menu">BOM</span></a>
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="PurchaseDetailsReport">  <span class="menu">Purchase Details</span></a>
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="CommodityPriceMovementReport">  <span class="menu">Commodity Price Movement </span></a>
                    <a class="nav-link" asp-area="" asp-controller="Home" asp-action="ExchangeRateReport">  <span class="menu">Exchange Rate Report </span></a>

                </div>
            </div>
           

            <div class="menu" style="pointer-events: none;"></div>
        </div>

        <div class="foot">
            <div class="profile">
                <img src="~/images/profile.jpg" />
               
                <div class="info">
                    <span class="username" id="username"></span>
                    <span class="userRoll" id="userRoll"></span>
                </div>
            </div>

            <div class="menu menu-logout" onclick="logout()">
                <i class="bx bx-log-out"></i>
                <span>Logout</span>
            </div>

           
        </div>

    </aside>

    <div class="body-main" >
        
            <div class="row">
                <div class="headtitle" >
                <div class="col-md-6" >
                    <h3 class="dashboard-title"></h3>
                </div>

                    <div class="col-md-6 d-flex justify-content-end">
                        <div class="r-btns" align="right">
                       
                            <div class="btn btn-primary btn-margin" id="save" onclick="fnvalidate()">
                                <i class="fa fa-plus-circle"></i><span class="iexp">Save</span>
                            </div>
                            <div class="btn btn-primary btn-margin" id="view" onclick="fnview()">
                                <i class="fa fa-search-plus"></i><span class="iexp">View</span>
                            </div>
                            <div class="btn btn-primary btn-margin" id ="clear" onclick="clearform()">
                                <i class="fa fa-eraser"></i><span class="iexp">Clear</span>
                            </div>

                      
                       
                        </div>
                    </div>

                </div>
            </div>
            <hr class="custom-hr">
            <div class="loader" id="loadingGear"></div>

        @RenderBody()
       
            <div class="modal fade" id="messageModal" tabindex="-1" role="dialog" aria-labelledby="messageModalLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="messageModalLabel">Message</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body" id="messageModalBody">
                            <!-- Message content will be displayed here -->
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
   
        
    </div>
    

   
    <!-- Modal Dialog -->
    
<!-- Your custom JavaScript files -->
   <!-- jQuery -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>


    <script src="~/js/jquery.inputpicker.js"></script>
    
<!-- Popper.js -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/2.9.3/umd/popper.min.js"></script>

<!-- Bootstrap JS -->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    
    <!-- DataTables -->
    <script src="https://cdn.datatables.net/2.0.3/js/dataTables.js"></script>
    <script src="https://cdn.datatables.net/fixedcolumns/5.0.0/js/dataTables.fixedColumns.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/3.0.1/js/dataTables.buttons.js"></script>
    <script src="https://cdn.datatables.net/buttons/3.0.1/js/buttons.dataTables.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.10.1/jszip.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/3.0.1/js/buttons.html5.min.js"></script>
    <script src="/_framework/aspnetcore-browser-refresh.js"></script>


    <script src="~/js/common.js"></script>
    <script src="~/js/masters.js"></script>
    <script src="~/js/template.js"></script>
    <script src="~/js/masterimport.js"></script>
</body>

</html>




