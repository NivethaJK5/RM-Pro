
<div class="con-main">

    <input type="hidden" class="form-control" value="ProductMaster" id="pgid">
    <input type="hidden" class="form-control" value="Master" id="pggrpid">
    <form id="myForm">

        <div class="row">
            <div class="form-row">
                <div class="form-group col-md-3">
                    <label for="Product_Code">Product Code</label>
                    <input type="text" class="form-control" id="Product_Code" placeholder="Product Code" required>
                </div>
                <div class="form-group col-md-3">
                    <label for="Product_Description">Product Description</label>
                    <input type="text" class="form-control" id="Product_Description" placeholder="Product Description" required>
                </div>

                <div class="form-group col-md-3">
                    <label for="Product_Category">Product Category</label>
                    <input type="text" class="form-control" id="Product_Category" placeholder="Product Category" required>
                </div>

                <div class="form-group col-md-3">
                    <label for="Product_Type">Product Type</label>
                    <input type="text" class="form-control" id="Product_Type" placeholder="Product Type" required>                
                </div>
                <div class="form-group col-md-3">
                    <label for="Bench_Mark">Bench Mark</label>
                    <select class="form-control" id="Bench_Mark">
                        <option value="Yes">Yes</option>
                        <option value="No">No</option>
                    </select>
                </div>
                <div class="form-group col-md-3">
                    <label for="PO_Base_Qty">PO Base Qty</label>
                    <input type="text" class="form-control" id="PO_Base_Qty" placeholder="PO Base Qty" min="0" required>
                </div>
                <div class="form-group col-md-3">
                    <label for="PO_Base_UOM">PO Base UOM</label>
                    <input type="text" class="form-control" id="PO_Base_UOM" placeholder="PO Base UOM" required>
                   
                </div>
            </div>
        </div>

    </form>
    <!-- Horizontal line -->
    <hr class="custom-hr">
    <h3>Recently Added</h3>
    <div id="masTableContainer">
    </div>
</div>





