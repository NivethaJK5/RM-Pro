
<div class="con-main">
      
    <input type="hidden" class="form-control" value="ProductCompositionMaster" id="pgid">
    <input type="hidden" class="form-control" value="Master" id="pggrpid">
    <form id="myForm">
       
            <div class="row">
                <div class="form-row">
                <!-- Model for Product_Code -->
                <div class="form-group col-md-3">
                    <label for="Product_Code">Product Code</label>
                    <input type="text" class="form-control" id="Product_Code" placeholder="Product Code" required>
                </div>

                <!-- Model for Base_Product -->
                <div class="form-group col-md-3">
                    <label for="Commodity">Commodity</label>
                    <input type="text" class="form-control" id="Commodity" placeholder="Commodity" required>
                </div>

                <!-- Model for Metal_Exchange -->
                <div class="form-group col-md-3">
                    <label for="Metal_Exchange">Metal Exchange</label>
                    <input type="text" class="form-control" id="Metal_Exchange" placeholder="Metal Exchange" required>
                    
                </div>

                <!-- Model for Percentage_to_Material -->
                <div class="form-group col-md-3">
                    <label for="Percentage_to_Material">Percentage to Material</label>
                    <input type="text" class="form-control" id="Percentage_to_Material" placeholder="Percentage to Material" min="0.0" required>
                </div>

                  
               
                </div>
            </div>

    </form>
